{
  "name": "Simple Scripts",
  "version": "0.1",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "borderify@mozilla.org",
      "strict_min_version": "45.0"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.simpleone.ru/record/*"],
      "js": ["hotkeys.js"]
    }
  ],
  "permissions": ["activeTab", "scripting"]
}
